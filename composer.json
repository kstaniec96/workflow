{
    "name": "simpler/simpler",
    "type": "project",
    "description": "The Simpler Framework",
    "keywords": ["framework", "Simpler"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "bepsvpt/secure-headers": "^7.2",
        "envms/fluentpdo": "^2.2",
        "filp/whoops": "^2.14",
        "firebase/php-jwt": "^6.1",
        "fzaninotto/faker": "^1.9",
        "jenssegers/agent": "^2.6",
        "monolog/monolog": "^2.5",
        "nesbot/carbon": "^2.57",
        "psr/log": "^3.0",
        "slim/php-view": "^3.1",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/var-dumper": "^5.4",
        "team-reflex/discord-php": "^7.0",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "files": [
          "app/src/helpers.php", "project/helpers.php"
        ],
        "psr-4": {
            "Database\\Migrations\\": "db/migrations/",
            "Database\\Fixtures\\": "db/fixtures/",
            "Database\\Factories\\": "db/factories/",
            "Project\\": "project/",
            "Simpler\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
